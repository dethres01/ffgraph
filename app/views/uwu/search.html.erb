<!-- app/views/travel/search.html.erb -->
<div class="d-flex justify-content-center">
  <%= link_to "GRAPH IT", uwu_graph_path(:log =>@report), class:"btn btn-dark mb-3"%>
  
</div>
<div class="d-flex justify-content-center">
  <h3> Log Overview</h3>
</div>
<% @logs.each do |fight|%>
    <div class= "container">
      <div class= "card bg-secondary mb-2">
        <h5 class="card-title">PULL NUMBER <%= fight['id']%></h5>
        <h6 class="card-subtitle mb-2"><%=fight['name']%></h6>
        <p class="card-text">Fight Remaining: <%= fight['fightPercentage']%> Last Phase: <%= fight['lastPhaseForPercentageDisplay']%> </p>
      </div>
    </div>
<%end%>

